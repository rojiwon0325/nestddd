{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/../src/$1",
    "@INTERFACE/(.*)$":"<rootDir>/../src/interface/$1",
    "@COMMON/(.*)$": "<rootDir>/../src/api/common/$1",
    "@USER/(.*)$": "<rootDir>/../src/api/user/$1"
  }
}
